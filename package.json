    "mkdirp": "0.5.1",
    "sax": "1.2.4"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "chai-string": "^1.4.0",
    "codecov": "3.0.2",
    "cz-conventional-changelog": "2.1.0",
    "dirty-chai": "2.0.1",
    "mocha": "5.2.0",
    "nyc": "12.0.2",
    "semantic-release": "^15.5.2",
    "standard": "11.0.1"
  },
  "scripts": {
    "lint": "standard",
